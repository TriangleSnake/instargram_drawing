{
  "manifest_version": 3,
  "name": "instagram Drawer",
  "version": "1.0",
  "description": "抽獎",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "version_name": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.instagram.com/*"],
      "js": ["content.js"]
    }
  ]

}
